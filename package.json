{
  "name": "noted",
  "version": "0.1.2",
  "description": "Personal document search engine",
  "repository": "https://github.com/radekstepan/noted",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "coverage": "npm test -- --coverage",
    "run-app": "react-scripts start",
    "run-server": "node server/",
    "run-server-dev": "node server/watch.js",
    "start": "concurrently 'npm:run-app' 'npm:run-server'",
    "dev": "concurrently 'npm:run-app' 'npm:run-server-dev' 'docker-compose -f docker-compose.yml -f docker-compose.dev.yml up'",
    "test": "CI=true react-scripts test --env=jsdom",
    "watch-test": "react-scripts test --env=jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "0.19.0",
    "chrono-node": "1.3.11",
    "concurrently": "4.1.1",
    "express": "4.17.3",
    "hasha": "5.0.0",
    "multer": "1.4.1",
    "nocache": "2.1.0",
    "node-sass": "4.12.0",
    "nodemon": "1.19.1",
    "normalize.css": "8.0.1",
    "p-iteration": "1.1.8",
    "react-scripts": "3.0.1",
    "serialize-error": "4.1.0",
    "slugify": "1.3.4",
    "smart-truncate": "1.0.1",
    "to-case": "2.0.0"
  },
  "devDependencies": {
    "@rematch/core": "1.1.0",
    "classnames": "2.2.6",
    "debounce": "1.2.0",
    "deep-diff": "1.0.2",
    "fs-extra": "8.1.0",
    "history": "4.9.0",
    "javascript-time-ago": "2.0.1",
    "p-whilst": "2.1.0",
    "path-to-regexp": "3.0.0",
    "querystringify": "2.1.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-dropzone": "10.1.5",
    "react-paginating": "1.3.0-hook.2",
    "react-perfect-scrollbar": "1.5.3",
    "react-pose": "4.0.8",
    "react-redux": "7.1.0",
    "react-scrollbar": "0.5.6"
  },
  "proxy": "http://0.0.0.0:9000"
}
