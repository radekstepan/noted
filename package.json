{
  "name": "noted",
  "version": "0.1.0",
  "description": "Personal document search engine",
  "repository": "https://github.com/radekstepan/noted",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "coverage": "npm test -- --coverage",
    "run-app": "react-scripts start",
    "run-server": "node server/",
    "start": "concurrently 'npm:run-*'",
    "start-dev": "ES_HOST='0.0.0.0' concurrently 'npm:run-*' 'docker-compose -f docker-compose.yml -f docker-compose.dev.yml up'",
    "test": "CI=true react-scripts test --env=jsdom",
    "watch-test": "react-scripts test --env=jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "0.18.0",
    "chrono-node": "1.3.11",
    "concurrently": "4.1.0",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "hasha": "4.0.1",
    "multer": "1.4.1",
    "nocache": "2.0.0",
    "node-sass": "4.11.0",
    "nodemon": "1.18.10",
    "normalize.css": "8.0.1",
    "p-iteration": "1.1.8",
    "react-scripts": "2.1.8",
    "serialize-error": "4.0.0",
    "smart-truncate": "1.0.1"
  },
  "devDependencies": {
    "@rematch/core": "1.1.0",
    "classnames": "2.2.6",
    "debounce": "1.2.0",
    "deep-diff": "1.0.2",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "history": "4.9.0",
    "javascript-time-ago": "2.0.1",
    "p-whilst": "2.0.0",
    "path-to-regexp": "3.0.0",
    "querystringify": "2.1.1",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-dropzone": "10.0.6",
    "react-paginating": "1.3.0-hook.1",
    "react-perfect-scrollbar": "1.4.4",
    "react-pose": "4.0.8",
    "react-redux": "6.0.1",
    "react-scrollbar": "0.5.4",
    "slugify": "1.3.4"
  },
  "proxy": "http://0.0.0.0:9000"
}
